
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-1.1.x"]
}

model Trademark {
  id           Int           @id @default(autoincrement())
  denomination String
  class        Int
  type         TrademarkType?
  certificate  String
  expiration   DateTime
  products     String?
  createdAt    DateTime      @default(now())
  updatedAt    DateTime      @updatedAt
  owner        Owner         @relation(fields: [ownerId], references: [id], onDelete: Cascade)
  ownerId      Int
}

model Owner {
  id         Int         @id @default(autoincrement())
  name       String      @unique
  country    Country
  createdAt  DateTime    @default(now())
  updatedAt  DateTime    @updatedAt
  trademarks Trademark[]
  contacts   Contact[]   @relation("OwnerContacts")
}

model Contact {
  id         Int         @id @default(autoincrement())
  firstName  String?
  lastName   String?
  email      String      @unique
  createdAt  DateTime    @default(now())
  updatedAt  DateTime    @updatedAt
  agent      Agent       @relation(fields: [agentId], references: [id], onDelete: Cascade)
  agentId    Int
  owners     Owner[]     @relation("OwnerContacts")
  sentEmails SentEmail[]
}

model Agent {
  id        Int       @id @default(autoincrement())
  name      String    @unique
  country   Country
  area      String?
  createdAt DateTime  @default(now())
  updatedAt DateTime  @updatedAt
  contacts  Contact[]
}

model EmailTemplate {
  id          Int        @id @default(autoincrement())
  name        String     @unique
  subject     String
  body        String
  createdAt   DateTime   @default(now())
  updatedAt   DateTime   @updatedAt
  campaigns   Campaign[]
}

model Campaign {
  id              Int           @id @default(autoincrement())
  name            String
  createdAt       DateTime      @default(now())
  emailTemplate   EmailTemplate @relation(fields: [emailTemplateId], references: [id], onDelete: Cascade)
  emailTemplateId Int
  sentEmails      SentEmail[]
}

model SentEmail {
  id         Int      @id @default(autoincrement())
  resendId   String   @unique
  status     String
  sentAt     DateTime @default(now())
  campaign   Campaign @relation(fields: [campaignId], references: [id], onDelete: Cascade)
  campaignId Int
  contact    Contact  @relation(fields: [contactId], references: [id], onDelete: Cascade)
  contactId  Int
}

enum TrademarkType {
  WORD
  FIGURATIVE
  MIXED
}

